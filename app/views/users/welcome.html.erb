          <div class='sign-in container'>
              <h2><%= image_tag 'logo', :class => 'img-responsive' %></h2>
          </div>
        </header>


        <!-- Fixed Height Image Aside -->
        <!-- Image backgrounds are set within the full-width-pics.css file. -->
        <aside class="image-bg-fixed-height">
          <%= render :partial => '/slideshows/slideshow_list_sect', :locals => { :slideshows => Slideshow.all, :page => 'general' } %>
        </aside>

        <aside class="image-bg-fixed-height-footer">
          <!-- Content Section -->
          <section>
            <div class="container">
              <div class="row">
                <div class="col-lg-12">
                  <h1 class="section-heading">Quick Studies</h1>
                  <p class="lead section-lead">Thank you for choosing Daily Quicks for your art reference needs.</p>
                  <p class="section-paragraph">No membership nessesary. Select one of the current slideshows above to get started with a study.</p>
                </div>
              </div>
              <!-- /.row -->
            </div>
            <!-- /.container -->
          </section>


        </aside>

